[bumblebee-nonfree-shortlived]
name=bumblebee nonfree for fedora Linux $releasever - $basearch - Base
baseurl=
	http://install.linux.ncsu.edu/pub/yum/itecs/public/bumblebee-nonfree-shortlived/fedora$releasever/
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-bumblebee-nonfree-shortlived-public

[bumblebee-nonfree-shortlived-source]
name=bumblebee nonfree for fedora Linux $releasever - $basearch - Source
baseurl=
	http://install.linux.ncsu.edu/pub/yum/itecs/public/bumblebee-nonfree-shortlived/fedora$releasever/SRPMS
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-bumblebee-nonfree-shortlived-public
gpgcheck=1
